<% provide(:title, 'Edit building') %>
<h1>Update building</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for([@landlord, @building]) do |f| %>
      <%= render 'fields', f: f %>
      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>  
